import json
import boto3

dynamodb = boto3.resource('dynamodb')
table = dynamodb.Table('Info_Table')

def lambda_handler(event, context):
    table.delete_item(
        Key={
            'ID':'1870'
        }
    )
    response = {
        'message': 'Item Deleted'
    }
    return {
        'statusCode':200,
        'body': response
    }
    

